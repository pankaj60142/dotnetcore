        padding: 10px 0;
        text-align: center;
    }

    sup.new {
        color: red;
    }

    path {
        stroke: white;
        stroke-width: 0.25px;
        fill: lightblue;
        cursor: pointer;
    }


    div.tooltipdatacenter {
        position: absolute;
        text-align: center;
        width: 190px;
        height: 205px;
        padding: 2px;
        font: 12px sans-serif;
        background: lightblue;
        border: 0px;
        border-radius: 8px;
        pointer-events: none;
    }

    div.tooltip {
        color: black;
        background: #fff;
        border-radius: 3px;
        box-shadow: 0px 0px 2px 0px #a6a6a6;
        padding: .2em;
        text-shadow: #f5f5f5 0 1px 0;
        opacity: 0.9;
        position: absolute;
        font-size: 14px;
        font-family: "Arial", sans-serif;
        font-weight: normal;
    }


    .names {
        fill: none;
        stroke: #fff;
        stroke-linejoin: round;
    }


    /*    text{
      pointer-events:none;
    }*/

    .details {
        color: white;
    }


