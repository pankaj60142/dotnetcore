        fill: #fff;
        stroke: orange;
        stroke-width: 2px;
        width: 10px;
        height: 10px;
    }

    .node text {
        font: 12px sans-serif;
        font-weight: normal;
    }

    .link {
        fill: none;
        stroke: black;
        stroke-width: 1px;
        stroke-dasharray: 20,10,5,5,5,10;
    }

    .nodedata circle {
        fill: #fff;
        stroke: steelblue;
        stroke-width: 3px;
    }

    .nodedata text {
        font: 12px sans-serif;
    }

    .linkdata {
        fill: none;
        stroke: #ccc;
        stroke-width: 2px;
