        position: relative;
        overflow: hidden;
        max-height: 21px;
        -webkit-transition: max-height 250ms ease-in;
        transition: max-height 250ms ease-in;
        -moz-transition: max-height 250ms ease-in;
    }

        .options pre {
            margin-top: 10px;
        }

        .options:hover {
            background-color: #DDD;
            cursor: pointer;
        }

        .options pre:hover {
            cursor: default;
        }

