                };
                options.complete = function (msg) {
                    if (msg.responseText == "SUCCESS") {

                        swal("Saved!", "Framework application saved successfully!", "success");

                    }
                    else
                        swal("Error", "Please fill in the required fields!", "error");
                };
                $.ajax(options);
            }

            }
            
        
