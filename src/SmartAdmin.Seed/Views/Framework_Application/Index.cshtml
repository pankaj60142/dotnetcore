        function addContactData() {
            if ($("#application_contact").valid()) {
                var FirstName = $('#FirstName').val();

                var LastName = $('#LastName').val();

                var MiddleInitial = $('#MiddleInitial').val();

                var Email = $('#Email').val();

                var Phone = $('#Phone').val();

                var Title = $('#Title').val();

