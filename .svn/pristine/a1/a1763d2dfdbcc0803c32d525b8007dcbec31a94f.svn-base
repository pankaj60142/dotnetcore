                                        <label class="label">Applications </label>
                                        <div class="input">

                                            <input type='text' id="applicationname" placeholder='Select Applications' class='flexdatalist' data-min-length='1' multiple='multiple' list='applications' name='datacenters1'>




                                        </div>
                                    </section>


