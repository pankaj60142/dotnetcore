    div.tooltip {
        color: black;
        background: #fff;
        border-radius: 3px;
        box-shadow: 0px 0px 2px 0px #a6a6a6;
        padding: .2em;
        text-shadow: #f5f5f5 0 1px 0;
        opacity: 0.9;
        position: absolute;
        font-size: 14px;
        font-family: "Arial", sans-serif;
        font-weight: normal;
    }


    .names {
        fill: none;
        stroke: #fff;
        stroke-linejoin: round;
    }

  
    /*    text{
      pointer-events:none;
    }*/

    .details {
        color: white;
    }


    .node circle {
        fill: #fff;
        stroke: orange;
        stroke-width: 2px;
        width: 10px;
        height: 10px;
    }

    .node text {
        font: 12px sans-serif;
        font-weight:normal;
    }

    .link {
        fill: none;
        stroke: black;
        stroke-width: 1px;
        stroke-dasharray: 20,10,5,5,5,10;
    }
</style>


