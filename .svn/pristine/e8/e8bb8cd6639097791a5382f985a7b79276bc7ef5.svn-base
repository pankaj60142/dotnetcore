@section Scripts {
    <script type="text/javascript">




        pageSetUp();

        // pagefunction

        var pagefunction = function () {

            var responsiveHelper_dt_basic = undefined;


            var breakpointDefinition = {
                tablet: 1024,
                phone: 480
            };
            // PAGE RELATED SCRIPTS

            var errorClass = 'invalid';
            var errorElement = 'em';




        };


        $(document).ready(function () {
            $("#InstalledDate").datepicker('setDate', 'today');
            $("#SupportExpirationDate").datepicker('setDate', 'today');

            Application_Database();
        });
        var arr = new Array();
        function addContactData() {
            var FirstName = $('#FirstName').val();
            var LastName = $('#LastName').val();
            var MiddleInitial = $('#MiddleInitial').val();
            var Email = $('#Email').val();
            var Phone = $('#Phone').val();
            var Title = $('#Title').val();
            var SID = $('#SID').val();
            var EmployeeNo = $('#EmployeeNo').val();
            var ContactIsValid = $('#ContactIsValid').val();
            var ContactTypeID = $('#ContactTypeID').val();
            var LoginName = $('#LoginName').val();
            
            var obj = {

                FirstName: FirstName,
                LastName: LastName,
                MiddleInitial: MiddleInitial,
                Email: Email,
                Phone: Phone,
                Title: Title,
                SID: SID,
                EmployeeNo: EmployeeNo,
                LoginName:LoginName,
                //ContactIsValid: ContactIsValid,
                ContactTypeID: ContactTypeID
            };

           
            arr.push(obj);
           
            $('#tblapplicationcontact').DataTable().destroy();
            $('#tblapplicationcontact').DataTable({
                data: JSON.parse(JSON.stringify(arr)),
                columns: [
                    { data: "FirstName" },
                    { data: "LastName" },
                    { data: "MiddleInitial" },
                    { data: "Email" },
                    { data: "Phone" },
                    { data: "Title" },
                    { data: "LoginName" },
                    { data: "SID" },
                    { data: "EmployeeNo" },
                    {
                        data: null,
                        className: "center",
                        defaultContent: '<button id="btnEdit" type="button" value="Edit">Edit</button><button id="btndelete" type="button" value="Delete">Delete</button>'
                    }
                  //{ title: "ContactIsValid" },
                    //{
                    //    data: 'Email',
                    //    'render': function (data, type, row) {

                    //        return '<input id="btnEdit" type="button" value="Edit"/>';
                    //    }
                    
                ]
            });
        }
            
           
        
        

        function Application_Database() {
            
               
                    var options = {};
            options.url = "/Framework_Application/Application_Database";
                    options.type = "GET";
                    var obj = {
                        
                    };

                    options.data = JSON.stringify(obj);
                    options.contentType = "application/json";
                    options.dataType = "json";

                    options.beforeSend = function (xhr) {
                        xhr.setRequestHeader("MY-XSRF-TOKEN",
                            $('input:hidden[name="__RequestVerificationToken"]').val());
                    };
            options.complete = function (msg) {
               // console.log(msg);

                for (var i = 0; i < msg.responseJSON.length; i++) {
                    var option = $('<option></option>').text(msg.responseJSON[i].DatabaseID);
                        $('#Application_Database').append(option);
}
               
                    };
                    $.ajax(options);

                
             }

        function SaveDataapplication() {
            if ($("#frm-application").valid()) {
                var Name = $('#Name').val();
                
                var Ver = $('#Version').val();
                var ShortDescription = $('#ShortDescription').val();
                var LongDescription = $('#LongDescription').val();
                var InstallationPath = $('#InstallationPath').val();
                var InstalledDate = $('#InstalledDate').val();
                var SupportPhone = $('#SupportPhone').val();
                var SupportEmail = $('#SupportEmail').val();
                var SupportAccountNo = $('#SupportAccountNo').val();
                var SupportExpirationDate = $('#SupportExpirationDate').val();
                var SupportURL = $('#SupportURL').val();
                var NumberOfLicenses = $('#NumberOfLicenses').val();
                var Comment = $('#Comment').val();
                var InstallerNameID = $('#InstallerNameID').val();
                var Usrname = $('#Username').val();
                var Pass = $('#Password').val();

                var DeveloperTypeID = $('#DeveloperTypeID').val();
                var DeveloperID = $('#DeveloperID').val();
                var CitrixApplicationName = $('#CitrixApplicationName').val();
                var ApplicationURL = $('#ApplicationURL').val();
                var ApplicationTypeID = $('#ApplicationTypeID').val();
                var IsVisibleInsideGGP = $('#IsVisibleInsideGGP').val();
                var CertificateExpiration = $('#CertificateExpiration').val();
                var SMTP = $('#SMTP').val();
                var FirewallException = $('#FirewallException').val();
                var VProcessDependent = $('#VProcessDependent').val();
                var LDAP = $('#LDAP').val();
                var options = {};
                options.url = "/Framework_Application/SaveData";
                options.type = "POST";
                var obj = {

                    Name: Name,
                    Ver: Ver,
                    ShortDescription: ShortDescription,
                    LongDescription: LongDescription,
                    InstallationPath: InstallationPath,
                    InstalledDate: InstalledDate,
                    SupportPhone: SupportPhone,
                    SupportEmail: SupportEmail,
                    SupportAccountNo: SupportAccountNo,
                    SupportExpirationDate: SupportExpirationDate,
                    SupportURL: SupportURL,
                    NumberOfLicenses: NumberOfLicenses,
                    Comment: Comment,
                    InstallerNameID: InstallerNameID,
                    Usrname: Usrname,
                    Pass: Pass,
                    DeveloperTypeID: DeveloperTypeID,
                    DeveloperID: DeveloperID,
                    CitrixApplicationName: CitrixApplicationName,
                    ApplicationURL: ApplicationURL,
                    ApplicationTypeID: ApplicationTypeID,
                    IsVisibleInsideGGP: IsVisibleInsideGGP,
                    CertificateExpiration: CertificateExpiration,
                    SMTP: SMTP,
                    ////IsVisibleNonEmployee: autoopen,
                    FirewallException: FirewallException,
                    VProcessDependent: VProcessDependent,
                    LDAP: LDAP

                };

                options.data = JSON.stringify(obj);
                options.contentType = "application/json";
                options.dataType = "json";

                options.beforeSend = function (xhr) {
                    xhr.setRequestHeader("MY-XSRF-TOKEN",
                        $('input:hidden[name="__RequestVerificationToken"]').val());
                };
                options.complete = function (msg) {
                    if (msg.responseText == "SUCCESS") {

                        swal("Saved!", "Framework application saved successfully!", "success");



                    }
                    else
                        swal("Error", "Please fill in the required fields!", "error");
                };
                $.ajax(options);

            }
            

            }
            
          

        function FillCities(stateid) {

            $.ajax({
                url: '/Framework_Application/GetCityName',
                type: 'POST',
                datatype: 'json',
                data: { StateId: stateid },
                error: function (jqXHR, textStatus, errorThrown) {
                    alert(errorThrown);

                },

                success: function (resultCity) {

                    var flexdatalistCity = $("#cityname").flexdatalist({
                        minLength: 1,
                        valueProperty: 'CityName',
                        visibleProperties: ["CityName", "Latitude", "Longitude", "CityId"],
                        searchIn: 'CityName',
                        data: resultCity
                    });

                    flexdatalistCity.on('select:flexdatalist', function (event, data) {
                        selectedcityid = data.CityId;
                        FillDataCenter(selectedcityid);

                    });

                }


            });
        }
        function FillDataCenter(cityid) {

            $.ajax({
                url: '/Framework_Application/GetDataCenterName',
                type: 'POST',
                datatype: 'json',
                data: { CityId: cityid },
                error: function (jqXHR, textStatus, errorThrown) {
                    alert(errorThrown);

                },

                success: function (resultDataCenter) {


                    var flexdatalistDatacenter = $("#datacentername").flexdatalist({
                        minLength: 1,
                        valueProperty: 'DataCenterName',
                        visibleProperties: ["DataCenterName", "DataCenterId"],

                        searchIn: 'DataCenterName',
                        data: resultDataCenter
                    });

                    flexdatalistDatacenter.on('select:flexdatalist', function (event, data) {

                        selecteddatacenterid = data.DataCenterId;
